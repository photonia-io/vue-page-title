{
  "name": "vue-page-title",
  "version": "1.3.0",
  "description": "Vue.js html/page title manager",
  "repository": "https://github.com/vinicius73/vue-page-title",
  "author": "Vinicius Reis <luiz.vinicius73@gmail.com>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "vue",
    "head",
    "title",
    "document",
    "meta"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "vite build && yarn build:tsc",
    "build:tsc": "tsc lib/index.ts --declaration --emitDeclarationOnly --lib es2022,DOM --outDir dist ",
    "dev": "vite dev"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "mini-svg-data-uri": "^1.4.4",
    "radash": "^7.0.0",
    "reseter.css": "^2.0.0",
    "trianglify": "^4.1.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.40.1"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "packageManager": "yarn@3.2.3"
}
